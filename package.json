{
  "name": "travelplanner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:design-system": "eslint src/design-system --config src/design-system/eslint.config.mjs",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,mjs,cjs,cts,mts,json,css,scss,md}\" --write",
    "format:check": "prettier \"**/*.{js,jsx,ts,tsx,mjs,cjs,cts,mts,json,css,scss,md}\" --check",
    "test": "vitest",
    "test:run": "vitest run",
    "test:changed": "vitest --run --changed",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:generate": "dotenv -e .env.local -- drizzle-kit generate",
    "db:migrate": "dotenv -e .env.local -- drizzle-kit migrate",
    "db:push": "dotenv -e .env.local -- drizzle-kit push"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "mapbox-gl": "^2.15.0",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-datepicker": "^8.7.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-intersection-observer": "^9.16.0",
    "react-map-gl": "^7.1.7",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^9.1.8",
    "@storybook/addon-docs": "^9.1.8",
    "@storybook/addon-onboarding": "^9.1.8",
    "@storybook/addon-vitest": "^9.1.8",
    "@storybook/nextjs-vite": "^9.1.8",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-storybook": "^9.1.8",
    "husky": "^8.0.0",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.4",
    "pg": "^8.16.3",
    "playwright": "^1.55.1",
    "prettier": "^3.6.2",
    "storybook": "^9.1.8",
    "stylelint": "^16.24.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs,cts,mts,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
